  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.16.0",
    "@parcel/transformer-glsl": "^2.16.0",
    "@parcel/transformer-jsonld": "^2.16.0",
    "@parcel/transformer-sass": "^2.16.0",
    "parcel": "latest"
  },
  "dependencies": {
    "gsap": "^3.7.0",
    "three": "^0.129.0"
  }
}
