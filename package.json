{
  "name": "akkistech-website",
  "version": "1.0.0",
  "description": "akkistech corporate website with WebGL shader effects",
  "scripts": {
    "start": "parcel src/*.html --open",
    "clean": "rm -rf dist/*",
    "build:parcel": "parcel build src/*.html --no-content-hash --no-source-maps --public-url ./",
    "build": "npm run clean && npm run build:parcel"
  },
  "repository": {
